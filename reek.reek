Duplication:
  max_calls: 3
  exclude:
    - host_connection

UncommunicativeVariableName:
  accept:
    - "_"
    - "e"

LongMethod:
  max_statements: 5
  exclude:
    - deploy
    - run$
    - run_command$

UtilityFunction:
  exclude:
    - Git
    - Time
    - release_name
    - shell_say

DataClump:
  exclude:
    - Log

FeatureEnvy:
  exclude:
    - Git